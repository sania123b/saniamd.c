#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter0 your code here. Read input from STDIN. Print output to STDOUT */    
    char s[1001];
    fgets(s,sizeof(s),stdin);
   // int i=0;
    int c=0;
     for(int i=0;s[i]!='\0';i++)
      {
         if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U' )
             {
             //printf("Yes\n");
             c++;
             }
     }
        /* else{
             printf("No\n");
             }
       // }*/
    if(c==strlen(s))
    {
        printf("Yes");
    }
    else{
        printf("No");
    }
    return 0;
}
