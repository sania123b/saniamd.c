#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,k;
    scanf("%d%d",&n,&k);
    int a[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int index=-1;
    for(int i=0;i<n;i++)
    {
       // index=-1;
        if(a[i]==k)
        {
            index=i;
            break;
        }
    }
        printf("%d",index);
    
    return 0;
}
